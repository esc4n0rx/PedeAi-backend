{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "baseUrl": "./src",
      "types": ["vitest/globals"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  